            <!-- ConsentController -->
			<trans-unit id="consentController.message.confirmed">
				<source>Thank you very much for your consent.</source>
				<target>Herzlichen Dank für Ihre Einwilligung.</target>
			</trans-unit>
			<trans-unit id="consentController.message.deleted">
				<source>Thank you for your feedback. We will delete your personal data from our database as soon as possible.</source>
				<target>Danke für Ihre Rückmeldung. Wir werden Ihre personenbezogenen Daten schnellstmöglich aus unserem Datenbestand löschen.</target>
			</trans-unit>
			<trans-unit id="consentController.error.noAddressFound">
				<source>Oops! Unfortunately, we could not find the related data. Please contact our support.</source>
				<target>Upps! Leider konnten wir die zugehörigen Daten nicht finden. Bitte kontaktieren Sie unseren Support.</target>
			</trans-unit>
			<trans-unit id="consentController.error.alreadyConfirmed">
				<source>TThank you for your feedback. You have already informed us of your decision</source>
				<target>Danke für Ihre Rückmeldung. Sie haben uns Ihre Entscheidung bereits mitgeteilt.</target>
			</trans-unit>



			<!-- MailingController -->
			<trans-unit id="mailingController.message.saved">
				<source>Successfully saved.</source>
				<target>Erfolgreich gespeichert.</target>
			</trans-unit>
			<trans-unit id="mailingController.error.noMail">
				<source>You must first create a consent email.</source>
				<target>Sie müssen zuerst eine Einwilligungsmail anlegen.</target>
			</trans-unit>
			<trans-unit id="mailingController.error.wrongFileType">
				<source>The selected file is not a valid CSV file.</source>
				<target>Die ausgewählte Datei ist keine gültige CSV-Datei.</target>
			</trans-unit>
			<trans-unit id="mailingController.error.importFailed">
				<source>Unfortunately, the import failed: %s</source>
				<target>Der Import ist leider fehlgeschlagen: %s</target>
			</trans-unit>
			<trans-unit id="mailingController.warning.nothingImported">
				<source>No records were imported.</source>
				<target>Es wurden keine Datensätze importiert.</target>
			</trans-unit>
			<trans-unit id="mailingController.message.importSuccessful">
				<source>The import was successful. Imported records:</source>
				<target>Der Import war erfolgreich. Importierte Datensätze: %s</target>
			</trans-unit>
			<trans-unit id="mailingController.error.invalidEmail">
				<source>The e-mail address provided is not complete. Please check again.</source>
				<target>Die angegebene E-Mailadresse ist nicht vollständig. Bitte überprüfen Sie diese noch einmal.</target>
			</trans-unit>
			<trans-unit id="mailingController.message.testMailSent">
				<source>The e-mail was successfully sent to the test address.</source>
				<target>Die E-Mail wurde erfolgreich an die Test-Adresse versendet.</target>
			</trans-unit>
			<trans-unit id="mailingController.error.testMailNotSent">
				<source>The e-mail could not be sent to the test address.</source>
				<target>Die E-Mail konnte nicht an die Test-Adresse versendet werden.</target>
			</trans-unit>
			<trans-unit id="mailingController.error.pleaseReassure">
				<source>In order for the e-mail to be finally sent, you must confirm the sending. To do this, activate the checkbox below.</source>
				<target>Damit die E-Mail final versendet wird, müssen Sie den Versand bestätigen. Aktiven Sie dafür das Häkchen unten.</target>
			</trans-unit>
			<trans-unit id="mailingController.message.mailSent">
				<source>The selected email has been successfully marked for sending and will be sent shortly.</source>
				<target>Die ausgewählte E-Mail wurde erfolgsreich für den Versand markiert und wird in Kürze versendet.</target>
			</trans-unit>


